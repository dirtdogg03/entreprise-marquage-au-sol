# Configuration du site Entreprise Marquage au Sol
# Copier ce fichier en .env.local et remplir les valeurs

# Google Search Console (verification)
GOOGLE_SITE_VERIFICATION=

# Coordonnees entreprise (utilisees dans le schema JSON-LD)
NEXT_PUBLIC_COMPANY_NAME=Entreprise Marquage au Sol
NEXT_PUBLIC_COMPANY_PHONE=
NEXT_PUBLIC_COMPANY_EMAIL=
NEXT_PUBLIC_COMPANY_ADDRESS=
NEXT_PUBLIC_COMPANY_CITY=
NEXT_PUBLIC_COMPANY_POSTAL_CODE=

# URL du site en production
NEXT_PUBLIC_SITE_URL=https://entreprise-marquage-au-sol.fr

# API Email (pour le formulaire de contact)
# Resend, SendGrid, ou autre service
EMAIL_API_KEY=
EMAIL_FROM=
EMAIL_TO=
